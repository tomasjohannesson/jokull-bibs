@Article{jokull-65-p91-96,
  author   = {Bergur Einarsson and Oddur Sigurðsson},
  title    = {{Jöklabreytingar (Glacier variations) 1930–1970, 1970–1995, 1995–2013 og 2013–2014}},
  journal  = {Jökull},
