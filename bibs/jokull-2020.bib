@Article{jokull-70-p151,
  author   = {Maxwell Brown and Bryndís Brandsdóttir},
  title    = {{Leó Kristjánsson – in memorian}},
  journal  = {Jökull},
  year     = {2020},
  volume   = {70},
  pages    = {151},
  tpdoi    = {o},
  abstract = {Our friend and colleague, Leó Kristjánsson passed away on Friday 13th March. He was 76 years old. Leó had been fighting myelofibrosis (an uncommon type of bone marrow cancer) for the past two years. With a suppressed immune system, an infection led to his death. Leó pioneered research on the palaeomagnetism of the basaltic lava sequences of Iceland and the surrounding region, with applications to stratigraphy and the analysis of directions and NRMs in terms of variations in the geomagnetic field over the past 15 million years. For his contribution to this field he was elected an AGU fellow in 2002. ...},
}

